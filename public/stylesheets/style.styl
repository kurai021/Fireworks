html, body
  padding 0
  margin 0
  width 100%
  height 100%
  background #000
  overflow hidden
  background-size cover

#library
  display none

.tweet
  padding 10px
  border 2px solid grey
  background-color white
  margin-bottom 10px
  z-index 9999
  display none

canvas
  transform translateZ(0)
  height 100%
  width 100%
  z-index 1
